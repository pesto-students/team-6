{
  "name": "snake-game-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pretty": "prettier --write --tab-width 4 \"src/**/*.js\"",
        "eslint-fix": "eslint --fix \"src/**/*.js\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "prettier": "^2.2.0"
  }
}
